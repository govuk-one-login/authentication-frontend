# Miscellaneous variables
APP_ENV=local
ENVIRONMENT=development
SERVICE_DOMAIN=localhost

# Local Express session configuration
SESSION_EXPIRY=3600000
SESSION_SECRET=123456

# SmartAgent configuration for Support form submission - Ask for values
SMARTAGENT_API_KEY=
SMARTAGENT_API_URL=
SMARTAGENT_WEBFORM_ID=

# Local stub client options
VTR='["Cl","Cl.m"]'

# Feature switches
SUPPORT_MFA_OPTIONS=1
SUPPORT_AUTHORIZE_CONTROLLER=1
SUPPORT_ACCOUNT_INTERVENTIONS=1
SUPPORT_REAUTHENTICATION=1
NO_PHOTO_ID_CONTACT_FORMS=1
LANGUAGE_TOGGLE_ENABLED=1
SUPPORT_NEW_IPV_SPINNER=1
SUPPORT_HTTP_KEEP_ALIVE=0
PRIVACY_NOTICE_REDIRECT_ENABLED=1
USE_REBRAND=1
SHOW_WALLET_CONTACT_FORM=1

# Analytics
GA4_ENABLED=false
GOOGLE_ANALYTICS_4_GTM_CONTAINER_ID=
ANALYTICS_COOKIE_DOMAIN=localhost
VITAL_SIGNS_INTERVAL_SECONDS=360

